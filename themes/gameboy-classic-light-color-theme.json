  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#1a4a3a",
    "enum": "#306850",
    "enumMember": "#526525",
    "function.declaration": "#306850",
    "function.defaultLibrary": "#1a4a3a",
    "interface": "#306850",
    "parameter": {
      "fontStyle": "italic"
    },
    "property.readonly": "#3d4d1c",
    "type": "#1a4a3a",
    "variable.readonly": "#3d4d1c"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#526525"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#79970b"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#79970b"
      }
    },
    {
      "name": "Storage and modifiers",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#79970b"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "Classes and types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.name.type.interface",
        "entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#1a4a3a"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#0f380f"
      }
    },
    {
      "name": "Variable parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3d4d1c"
      }
    },
    {
      "name": "Variable language",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#79970b"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#526525"
      }
    },
    {
      "name": "Escape characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#79970b"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#526525"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#3d4d1c"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#79970b"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#79970b"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "constant.other.color.rgb-value.css"
      ],
      "settings": {
        "foreground": "#526525"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#79970b"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#526525"
      }
    },
    {
      "name": "Markdown List",
      "scope": [
        "markup.list",
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#79970b"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#526525"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1a4a3a"
      }
    },
    {
      "name": "Namespace",
      "scope": [
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#3d4d1c"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#79970b"
      }
    },
    {
      "name": "Ruby specific",
      "scope": [
        "punctuation.separator.inheritance.ruby",
        "punctuation.separator.method.ruby",
        "punctuation.separator.namespace.ruby",
        "punctuation.definition.constant.ruby",
        "punctuation.definition.constant.hashkey.ruby",
        "punctuation.separator.key-value",
        "punctuation.definition.variable.ruby",
        "keyword.control.pseudo-method.ruby"
      ],
      "settings": {
        "foreground": "#526525"
      }
    },
    {
      "name": "Ruby classes and modules",
      "scope": [
        "keyword.control.module.ruby",
        "keyword.control.class.ruby",
        "entity.other.inherited-class.ruby",
        "keyword.other.special-method.ruby",
        "entity.name.function.ruby"
      ],
      "settings": {
        "foreground": "#306850"
      }
    },
    {
      "name": "Ruby keywords and variables",
      "scope": [
        "keyword.control.def.ruby",
        "keyword.control.ruby",
        "support.class.ruby",
        "entity.name.type.class.ruby",
        "entity.name.type.module.ruby",
        "punctuation.section.function.ruby"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#79970b"
      }
    }
  ]
}
